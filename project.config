[project]
	description = mini python project for RPI 3
[access]
	inheritFrom = All-Projects
[access "refs/*"]
	owner = group user/Vasu (vazudew)
[access "refs/for/refs/*"]
	read = group user/Vasu (vazudew)
	push = group user/Vasu (vazudew)
	label-Code-Review = -2..+2 group user/Vasu (vazudew)
	label-Verified = -1..+1 group user/Vasu (vazudew)
	rebase = group user/Vasu (vazudew)
	removeReviewer = group user/Vasu (vazudew)
	submit = group user/Vasu (vazudew)
[access "refs/heads/*"]
	read = group user/Vasu (vazudew)
	create = group user/Vasu (vazudew)
	push = +force group user/Vasu (vazudew)
	pushMerge = group user/Vasu (vazudew)
[access "refs/tags/*"]
	push = group user/Vasu (vazudew)
	push = +force group user/Vasu (vazudew)
[receive]
	requireSignedOffBy = true
	requireChangeId = true
[submit]
	action = fast forward only
	mergeContent = false
[access "refs/for/master"]
	read = group vRebootAdmin
	abandon = group vRebootAdmin
	push = group vRebootAdmin
	label-Code-Review = -2..+2 group vRebootAdmin
	label-Verified = -1..+1 group user/vrebootadmin
	rebase = group vRebootAdmin
	submit = group vRebootAdmin
